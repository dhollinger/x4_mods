<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

  <xs:include schemaLocation="common.xsd" />

  <xs:simpleType name="gamestartimageid">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-z0-9_]+" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gamestartid">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-z0-9_]+" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gamestartmacroname">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-z0-9_]+" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gamestartmacropath">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-z0-9_./#]+" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gamestartconnectionname">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-z0-9_]+" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gamestartcustomvalue">
    <xs:restriction base="xs:string">
      <xs:enumeration value="galaxy" />
      <xs:enumeration value="sector" />
      <xs:enumeration value="zone" />
      <xs:enumeration value="stationpath" />
      <xs:enumeration value="stationmacro" />
      <xs:enumeration value="tradestation" />
      <xs:enumeration value="shipyard" />
      <xs:enumeration value="wharf" />
      <xs:enumeration value="equipmentdock" />
      <xs:enumeration value="defencestation" />
      <xs:enumeration value="piratebase" />
      <xs:enumeration value="headquarters" />
      <xs:enumeration value="shippath" />
      <xs:enumeration value="shipmacro" />
      <xs:enumeration value="roompath" />
      <xs:enumeration value="roommacro" />
      <xs:enumeration value="playerroom" />
      <xs:enumeration value="interiortype" />
      <xs:enumeration value="interiorname" />
      <xs:enumeration value="interiorcorridor" />
      <xs:enumeration value="interiordoor" />
      <xs:enumeration value="interiorroom" />
      <xs:enumeration value="interiorpersistent" />
      <xs:enumeration value="interiorprivate" />
      <xs:enumeration value="interiorrenderoutside" />
      <xs:enumeration value="modulepath" />
      <xs:enumeration value="modulemacro" />
      <xs:enumeration value="faction" />
      <xs:enumeration value="docked" />
      <xs:enumeration value="disembarked" />
      <xs:enumeration value="offset" />
      <xs:enumeration value="playeroffset" />
      <xs:enumeration value="player" />
      <xs:enumeration value="playername" />
      <xs:enumeration value="playermoney" />
      <xs:enumeration value="playerinventory" />
      <xs:enumeration value="playerblueprints" />
      <xs:enumeration value="playerstationblueprints" />
      <xs:enumeration value="playerbuildmethod" />
      <xs:enumeration value="playerresearch" />
      <xs:enumeration value="playerencyclopedia" />
      <xs:enumeration value="playertimeline" />
      <xs:enumeration value="playerknownfactions" />
      <xs:enumeration value="playerknownspace" />
      <xs:enumeration value="playerknownobjects" />
      <xs:enumeration value="playerpainttheme" />
      <xs:enumeration value="playerclothingtheme" />
      <xs:enumeration value="playerclothingmod" />
      <xs:enumeration value="playerproperty" />
      <xs:enumeration value="playerfemale" />
      <xs:enumeration value="ship" />
      <xs:enumeration value="shipname" />
      <xs:enumeration value="shiploadout" />
      <xs:enumeration value="shipcargo" />
      <xs:enumeration value="shippeople" />
      <xs:enumeration value="shippeoplefillpercent" />
      <xs:enumeration value="shippaintmod" />
      <xs:enumeration value="seed" />
      <xs:enumeration value="universeringhighways" />
      <xs:enumeration value="universelinearhighways" />
      <xs:enumeration value="universeregiondamagefactor" />
      <xs:enumeration value="universeobjectdensities" />
      <xs:enumeration value="universeyielddensities" />
      <xs:enumeration value="universeobjectyielddensities" />
      <xs:enumeration value="universeyieldreplenishtimedensities" />
      <xs:enumeration value="universehiddenobjectregionfactor" />
      <xs:enumeration value="universehiddenobjectstationfactor" />
      <xs:enumeration value="universejobquotafactors" />
      <xs:enumeration value="universegodquotafactors" />
      <xs:enumeration value="universefactionpolice" />
      <xs:enumeration value="universefactionrelations" />
      <xs:enumeration value="universestorystates" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="regionobjectclasslookup">
    <xs:restriction base="xs:string">
      <xs:enumeration value="asteroid" />
      <xs:enumeration value="mine" />
      <xs:enumeration value="lockbox" />
      <xs:enumeration value="object">
        <xs:annotation>
          <xs:documentation>
            Used for debris fields
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="positional">
        <xs:annotation>
          <xs:documentation>
            Used for nebula fields
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="knownobjectclasslookup">
    <xs:restriction base="xs:string">
      <xs:enumeration value="object">
        <xs:annotation>
          <xs:documentation>
            Any object
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="asteroid" />
      <xs:enumeration value="mine" />
      <xs:enumeration value="lockbox" />
      <xs:enumeration value="ship">
        <xs:annotation>
          <xs:documentation>
            Any ship
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ship_xs" />
      <xs:enumeration value="ship_s" />
      <xs:enumeration value="ship_m" />
      <xs:enumeration value="ship_l" />
      <xs:enumeration value="ship_xl" />
      <xs:enumeration value="satellite" />
      <xs:enumeration value="station" />
      <xs:enumeration value="buildstorage" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="regionwarelookup">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ore" />
      <xs:enumeration value="nividium" />
      <xs:enumeration value="silicon" />
      <xs:enumeration value="ice" />
      <xs:enumeration value="hydrogen" />
      <xs:enumeration value="helium" />
      <xs:enumeration value="methane" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="scenarioscoretype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="int" />
      <xs:enumeration value="float" />
      <xs:enumeration value="time" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="gamestartgroup">
    <xs:attribute name="group" type="xs:integer">
      <xs:annotation>
        <xs:documentation>
          Gamestart group number
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Group heading
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartgroups">
    <xs:sequence>
      <xs:element name="group" type="gamestartgroup" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Gamestart group heading definitions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="gamestartcustomstate">
    <xs:restriction base="xs:string">
      <xs:enumeration value="standard">
        <xs:annotation>
          <xs:documentation>
            Value can be customised and results in standard (unmodified) game
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="modified">
        <xs:annotation>
          <xs:documentation>
            Value can be customised but results in modified game
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="budgeted">
        <xs:annotation>
          <xs:documentation>
            Value can be customised using a budget and results in a standard (unmodified) game if within that budget or a modified game if outside it
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="gamestartextensionsource">
    <xs:attribute name="type" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>
          Extension source type
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="value" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>
          Extension source value
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartextension">
    <xs:sequence>
      <xs:element name="source" type="gamestartextensionsource" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Extension sources
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Extension ID
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="personal" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>
          Extension is personal (defaults to false)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartinfoitem">
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Item name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="value" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Item description
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartinfo">
    <xs:sequence>
      <xs:element name="item" type="gamestartinfoitem" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Definition of one name/description text pair
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="requirement" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Description of requirement to make this gamestart available
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="startday">
      <xs:annotation>
        <xs:documentation>
          Day of the month this gamestart starts in.
        </xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:positiveInteger">
          <xs:minInclusive value="1" />
          <xs:maxInclusive value="31" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="startmonth">
      <xs:annotation>
        <xs:documentation>
          Month this gamestart starts in.
        </xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:positiveInteger">
          <xs:minInclusive value="1" />
          <xs:maxInclusive value="12" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="startyear" type="xs:integer">
      <xs:annotation>
        <xs:documentation>
          Year (CE/AD) this gamestart starts in.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartdependencies">
    <xs:sequence>
      <xs:element name="dependency" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            A dependency defined by the child 'budget' nodes. If any defined budget in this set is currently selected then this dependency is fulfilled.
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="budget" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:attribute name="ref"/>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="gamestartunlockachievement">
    <xs:attribute name="ref" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Achievement name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartunlockuserdata">
    <xs:attribute name="ref" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>
          User data item name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="value" type="xs:integer" use="optional">
      <xs:annotation>
        <xs:documentation>
          User data value (default is 1)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mask" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>
          Compare user data value as bitmask (otherwise use default behaviour, which is to compare exact value)?
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="comparison" type="xs:integer">
      <xs:annotation>
        <xs:documentation>
          Value comparison (1 for greater or equal, -1 for less or equal, 0 for exactly equal, default is 0)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="progress" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>
          Whether this userdata should be taken into account for the overall progress shown in the UI. Defaults to false. Does not work if 'mask' is true.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>
          Text to show if this unlock requirement has not been met.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartunlock">
    <xs:sequence>
      <xs:element name="achievement" type="gamestartunlockachievement" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Achievement required to unlock gamestart
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="userdata" type="gamestartunlockuserdata" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            User data required to unlock gamestart
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="locked" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>
          Whether gamestart should locked regardless of conditions (false by default)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="hidden" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>
          Whether gamestart should be hidden until unlocked (false by default)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartcutscene">
    <xs:attribute name="ref" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>
          Gamestart cutscene name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="voice" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Gamestart cutscene voice sequence name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartintro">
    <xs:attribute name="ref" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>
          Gamestart intro animation
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="voice" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Gamestart intro animation voice sequence name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="length" type="xs:float">
      <xs:annotation>
        <xs:documentation>
          Length of the startmenu intro in s (defaults to 30s)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fadeout" type="xs:float">
      <xs:annotation>
        <xs:documentation>
          Length of the startmenu intro fade out in s (defaults to 2s)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestarttype">
    <xs:attribute name="name" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>
          Gamestart type name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="description" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Gamestart type description
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="recommendation" type="xs:integer">
      <xs:annotation>
        <xs:documentation>
          Gamestart recommendation score (higher is more recommended)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartloadingscreentext">
    <xs:attribute name="text" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Loadingscreen text
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartloadingscreen">
    <xs:sequence>
      <xs:element name="text" type="gamestartloadingscreentext" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Definition of loadingscreen text
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="info" type="gamestartloadingscreentext" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Definition of loadingscreen info text
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="ref" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Initial loading screen to be displayed for this gamestart.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          The text to be displayed on the gamestart specific loading screen. Empty, if no text should be displayed.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="length" type="xs:float">
      <xs:annotation>
        <xs:documentation>
          Time (in s) for how long the loading screen is to be displayed (0 implies only that screen being displayed).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartplayerlocation">
    <xs:sequence>
      <xs:element name="position" type="position" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Start position of player
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="rotation" type="rotation" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Start rotation of player
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="room" type="gamestartmacroname">
      <xs:annotation>
        <xs:documentation>
          Macro name of room
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="disembarked" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>
          Whether player is disembarked from player ship
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartinterior">
    <xs:attribute name="type" type="roomtypelookup">
      <xs:annotation>
        <xs:documentation>
          Interior type
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Interior name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="corridor" type="gamestartmacroname" use="required">
      <xs:annotation>
        <xs:documentation>
          Macro name of interior corridor
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="door" type="gamestartconnectionname" use="required">
      <xs:annotation>
        <xs:documentation>
          Connection name within corridor to which room should be attached
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="room" type="gamestartmacroname" use="required">
      <xs:annotation>
        <xs:documentation>
          Macro name of interior room
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="module" type="gamestartmacroname">
      <xs:annotation>
        <xs:documentation>
          Macro name of module within object in which to create interior
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="modulepath" type="gamestartmacropath">
      <xs:annotation>
        <xs:documentation>
          Macro path of module within object in which to create interior
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="persistent" type="boolean">
      <xs:annotation>
        <xs:documentation>
          Is the interior persistent in low attention? Otherwise, must be cleaned up by another system on attention change.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="private" type="boolean">
      <xs:annotation>
        <xs:documentation>
          Can NPCs find this interior to visit or spawn in?
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="renderoutside" type="boolean">
      <xs:annotation>
        <xs:documentation>
          Is this interior also rendered when the player is outside of it?
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="gamestartlocation">
    <xs:sequence>
      <xs:element name="position" type="position" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Start position of player ship
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="rotation" type="rotation" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Start rotation of player ship
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="player" type="gamestartplayerlocation" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Location of player if separate from ship
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="interior" type="gamestartinterior" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Dynamically created interior for player to start in
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="galaxy" type="gamestartmacroname" use="required">
      <xs:annotation>
        <xs:documentation>
          Macro name of galaxy to use for map
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sector" type="gamestartmacroname">
      <xs:annotation>
        <xs:documentation>
          Macro name of start sector within galaxy (ignored if start zone specified, random sector selected if neither specified)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="zone" type="gamestartmacroname">
      <xs:annotation>
        <xs:documentation>
          Macro name of start zone within galaxy
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stationpath" type="gamestartmacropath">
      <xs:annotation>
        <xs:documentation>
          Macro path of station within galaxy
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
