<?xml version="1.0" encoding="UTF-8"?>
<diff xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../xmllib/diff.xsd">
  <replace sel="//cues/cue[@name='Place_Claimable_Ships_SVE']">
    <conditions>
      <event_player_created/>
    </conditions>
    <delay exact="10s" />
    <actions>
      <!-- Heretics end -->
      <find_sector name="$sector" macro="macro.Cluster_31_Sector001_macro"/>
      <do_if value="$sector.exists">
        <generate_loadout
          macro="macro.baku_macro" faction="faction.ownerless"
          wares="[ware.ishield_arg_m_baku]"
          level="1"
          result="$baku_loadout"
        />
        <create_ship name="$ship" macro="macro.baku_macro" sector="$sector">
          <owner exact="faction.ownerless"/>
          <position x="-120km" y="7km" z="-67km"/>
          <rotation yaw="160deg" pitch="-50deg" roll="32deg"/>
          <loadout loadout="$baku_loadout.{1}" />
        </create_ship>
      </do_if>
      
      <!-- Matrix #451 -->
      <find_sector name="$sector" macro="macro.Cluster_16_Sector001_macro"/>
      <do_if value="$sector.exists">
        <generate_loadout
            macro="macro.taipan_macro" faction="faction.ownerless"
            wares="[ware.ishield_spl_l_taipan]"
            level="1"
            result="$taipan_loadout"
        />
        <create_ship name="$ship" macro="macro.taipan_macro" sector="$sector">
          <owner exact="faction.ownerless"/>
          <position x="100km" y="3km" z="-8km"/>
          <rotation yaw="-16deg" pitch="5deg" roll="12deg"/>
          <loadout loadout="$taipan_loadout.{1}" />
        </create_ship>
      </do_if>
      
      <!-- Turquoise Sea X -->
      <find_sector name="$sector" macro="macro.Cluster_50_Sector002_macro"/>
      <do_if value="$sector.exists">
        <generate_loadout
            macro="macro.razorbill_macro" faction="faction.ownerless"
            wares="[ware.ishield_tel_l_razorbill]"
            level="1"
            result="$razorbill_loadout"
        />
        <create_ship name="$ship" macro="macro.razorbill_macro" sector="$sector">
          <owner exact="faction.ownerless"/>
          <position x="-265km" y="-1km" z="-17km"/>
          <rotation yaw="6deg" pitch="5deg" roll="-12deg"/>
          <loadout loadout="$razorbill_loadout.{1}" />
        </create_ship>
      </do_if>
      
      <!-- Tharkas Ravine XVI -->
      <find_sector name="$sector" macro="macro.Cluster_410_Sector001_macro"/>
      <do_if value="$sector.exists">
         <generate_loadout
            macro="macro.scorpion_macro" faction="faction.ownerless"
            wares="[ware.ishield_spl_m_scorpion]"
            level="1"
            result="$scorpion_loadout"
        />
        <create_ship name="$ship" macro="macro.scorpion_macro" sector="$sector">
          <owner exact="faction.ownerless"/>
          <position x="-27km" y="1km" z="-3km"/>
          <rotation yaw="-7deg" pitch="13deg" roll="-52deg"/>
          <loadout loadout="$scorpion_loadout.{1}" />
        </create_ship>
      </do_if>
    </actions>
  </replace>
</diff>
